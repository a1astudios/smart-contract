pragma solidity ^0.6.0;

contract Counter {
    string public name;
    uint public count;
  

    constructor(string memory _name, uint _initialCount) {
        name = _name;
        count = _initialCount;
    }

    function increment() public returns (uint newCount)  {
        count =count ++;
        return count;


    }
}
